{
    "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "paths": {
        "@worker/*": ["apps/worker/src/*"],
        "@api/*": ["apps/api/src/*"],
        "@prisma/*": ["packages/prisma/*"]
      }
    }
  }
  